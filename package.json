{
  "name": "dabeers.be",
  "version": "1.0.0",
  "repository": "https://github.com/jrodl3r/dabeers.be",
  "author": "John Rodler",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "init": "mkdir public && mkdir functions/dist",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build && npm run copy:hosting && npm run build:ssr && npm run build:functions",
    "build:prod": "ng build --prod && npm run copy:hosting && npm run build:ssr && npm run build:functions",
    "build:functions": "npm run --prefix functions build",
    "build:ssr": "ng run ng-fire-universal:server && npm run webpack:ssr",
    "copy:hosting": "rm -r ./public && mkdir public && cp -r ./dist/ng-fire-universal/* ./public && rm ./public/index.html",
    "serve:ssr": "node dist/ng-fire-universal-webpack/server.js",
    "webpack:ssr": "webpack --config webpack.server.config.js",
    "firebase:review": "npm run build:prod && firebase serve --host 0.0.0.0",
    "firebase:deploy": "npm run build:prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~7.2.11",
    "@angular/common": "~7.2.11",
    "@angular/compiler": "~7.2.11",
    "@angular/core": "~7.2.11",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "~7.2.11",
    "@angular/http": "~7.2.11",
    "@angular/platform-browser": "~7.2.11",
    "@angular/platform-browser-dynamic": "~7.2.11",
    "@angular/platform-server": "~7.2.11",
    "@angular/pwa": "^0.13.7",
    "@angular/router": "~7.2.11",
    "@angular/service-worker": "~7.2.11",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "firebase": "^5.9.2",
    "ngx-toastr": "^10.0.2",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.7",
    "@angular/cli": "^7.3.7",
    "@angular/compiler-cli": "~7.2.11",
    "@angular/language-service": "~7.2.11",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.12.1",
    "codelyzer": "~5.0.0",
    "express": "^4.16.4",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "ts-loader": "^5.3.3",
    "ts-node": "~8.0.3",
    "tslint": "~5.14.0",
    "typescript": "^3.2.4",
    "webpack-cli": "^3.3.0",
    "ws": "^6.2.1",
    "xhr2": "^0.1.4"
  }
}
